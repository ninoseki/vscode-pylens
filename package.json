{
  "name": "vscode-pylens",
  "displayName": "pylens",
  "description": "Shows the latest version of a dependency in pyproject.toml or requirements.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/ninoseki/vscode-pylens.git"
  },
  "version": "0.2.4",
  "author": "Manabu Niseki",
  "publisher": "ninoseki",
  "license": "MIT",
  "engines": {
    "vscode": "^1.77.0"
  },
  "keywords": [
    "pyproject",
    "python"
  ],
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:pyproject.toml",
    "workspaceContains:requirements.txt"
  ],
  "contributes": {
    "commands": [
      {
        "title": "Enable pylens CodeLens",
        "command": "vscode-pylens.enableCodeLens"
      },
      {
        "title": "Disable pylens Codelens",
        "command": "vscode-pylens.disableCodeLens"
      }
    ],
    "configuration": {
      "properties": {
        "vscode-pylens.enableCodeLens": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "main": "./dist/extension.js",
  "icon": "images/logo.png",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src --ext ts",
    "dev": "npm run build --watch",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info",
    "vscode:prepublish": "npm run build",
    "publish": "vsce publish",
    "pack": "vsce package"
  },
  "devDependencies": {
    "@types/iarna__toml": "^2.0.2",
    "@types/jest": "29.5.1",
    "@types/node": "18.15.13",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "coveralls": "3.1.1",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "jest": "29.5.0",
    "prettier": "2.8.7",
    "ts-jest": "29.1.1",
    "tsup": "^6.7.0",
    "typescript": "5.0.4",
    "vscode-test": "1.6.1"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "axios": "^1.3.6",
    "axios-cache-interceptor": "^1.0.1",
    "semver": "^7.5.0"
  }
}
